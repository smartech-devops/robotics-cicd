{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowGitHubActionsPushPull",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::485701710361:role/robotics-cicd-github-actions-role"
      },
      "Action": [
        "ecr:GetDownloadUrlForLayer",
        "ecr:BatchGetImage",
        "ecr:BatchCheckLayerAvailability",
        "ecr:PutImage",
        "ecr:InitiateLayerUpload",
        "ecr:UploadLayerPart",
        "ecr:CompleteLayerUpload"
      ]
    }
  ]
}
